  "Toolchain": {
    "Preference": [
      "Win7/VS/v143/x64",
      "Win7/VS/v142/x64",
      "Win7/VS/v141/x64",
      "Win7/VS/v140/x64",
      "Win7/VS/v143/x64/H",
      "Win7/VS/v142/x64/H",
      "Win7/VS/v141/x64/H",
